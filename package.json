{
  "name": "reveal-md-style",
  "version": "1.0.0",
  "description": "A custom theme style for revealjs presentation generate from markdown with reveal-md",
  "scripts": {
    "sass": "sass --watch --no-source-map themes/scss:themes/css",
    "static": "reveal-md test.md --static",
    "serve": "reveal-md test.md -w --css themes/css/pastel.css --disable-auto-open",
    "all": "reveal-md all.md -w --css themes/css/list/pastel.css --preprocessor preproc.js --disable-auto-open",
    "list": "reveal-md list.md -w --css themes/css/list/pastel.css --preprocessor preproc.js --disable-auto-open",
    "merge": "cat ./preproc/*/* ./preproc/* > preproc.js"
  },
  "dependencies": {
    "reveal-md": "5.3.4"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "typescript": "4.9.4",
    "sass": "1.56.0"
  },
  "author": "Yann Zavattero",
  "license": "ISC"
}
